! Just set everything manually.
clear      lock
clear   control
clear      mod1
clear      mod2
clear      mod3
clear      mod4
clear      mod5
! Key 133 is the search key
keycode     133 = Control_L
! Key 37 is left control
keycode      37 = Super_L
! Key 105 is right control
keycode     105 = Overlay1_Enable
add     control = Control_L Control_R
add        mod1 = Alt_L Alt_R Meta_L
add        mod2 = Num_Lock
! add        mod3 =
add        mod4 = Super_L Super_R Hyper_L
add        mod5 = Mode_switch ISO_Level3_Shift
